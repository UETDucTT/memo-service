{
  "name": "memo-service",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:docker": "docker-compose -p cart-service-local up -d",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/mongoose": "^7.2.3",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.5.5",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/typeorm": "^7.1.4",
    "@nestjs/websockets": "^7.5.5",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.5",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.12.2",
    "cron": "^1.8.2",
    "date-fns": "^2.16.1",
    "ejs": "^3.1.5",
    "google-auth-library": "^6.1.1",
    "handlebars": "^4.7.6",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "mongoose": "^5.11.15",
    "mongoose-paginate-v2": "^1.3.14",
    "nodemailer": "^6.4.15",
    "passport-google-oauth20": "^2.0.0",
    "pino": "^6.7.0",
    "prom-client": "^12.0.0",
    "prometheus-api-metrics": "^3.1.0",
    "redlock": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/cheerio": "^0.22.23",
    "@types/express": "^4.17.3",
    "@types/ioredis": "^4.17.4",
    "@types/jest": "26.0.10",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^13.9.1",
    "@types/redlock": "^4.0.1",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
